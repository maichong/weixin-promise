{
  "extends": "airbnb",
  "env": {
    "node": true
  },
  "globals": {
    "wx": true
  },
  "parser": "babel-eslint",
  "rules": {
    "prefer-const": 0,
    "prefer-template": 0,
    "no-param-reassign": 0,
    "comma-dangle": [
      1,
      "never"
    ],
    "spaced-comment": [
      0,
      "always"
    ],
    "func-names": 0
  }
}
